--exclude-table-data='cmm_task_task_seq_seq'

//2023-15-06
pg_restore -h 127.0.0.1 -p 55432 -U us_fwd_dev -d us_fwd_dev -v ./dump-us_fwd_stg-us_fwd_dev-20230721.dump --clean --if-exists

psql -h 127.0.0.1 -p 55432 -U us_fwd_dev -d us_fwd_dev -f ./

pg_restore -h 127.0.0.1 -p 55432 -U us_fwd_dev -d us_fwd_dev -W mydump.sql

cat DC_us_fwd_stg_2023071716.sql | psql -h 127.0.0.1 -p 55432 -U us_fwd_dev

//Restore Ao DB postgres/Cyber2023
pg_restore -h 10.0.74.25 -p 54032 -U postgres -d us_fwd_dev -v --no-privileges --no-owner ./dump-us_fwd_dev-20230728.dump --clean --if-exists

