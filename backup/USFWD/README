pg_dump -h 10.0.68.203 -p 5432 -U us_fwd_dev -d us_fwd_test -F c -b -v -f dump-us_fwd_test-us_fwd_dev-20230801.dump


pg_dump -h 127.0.0.1 -p 55432 -U us_fwd_dev  --clean

