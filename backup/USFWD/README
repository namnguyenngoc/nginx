pg_dump -h 10.0.68.203 -p 5432 -U us_fwd_dev -d us_fwd_stg --exclude-table-data='cmm_task_task_seq_seq' -F c -b -v -f dump-us_fwd_stg-us_fwd_dev-20230727.dump
